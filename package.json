{
  "name": "jahia-testrail-reporter",
  "description": "Testrail reporter that accepts mocha json and jest xml reports",
  "version": "0.1.5",
  "author": "Vladislav Radan",
  "bin": {
    "jahia-testrail-reporter": "./bin/run"
  },
  "bugs": "https://github.com/VladRadan/jahia-testrail-reporter/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/node": "^10",
    "date-fns-timezone": "^0.1.4",
    "glob": "^7.1.6",
    "ts-node": "^8",
    "ts-sync-request": "^1.4.1",
    "tslib": "^1",
    "typescript": "^3.3",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "mocha": "^5",
    "nyc": "^14"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/VladRadan/jahia-testrail-reporter",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "jahia-testrail-reporter"
  },
  "repository": "VladRadan/jahia-testrail-reporter",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
