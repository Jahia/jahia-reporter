{
  "name": "@jahia/jahia-reporter",
  "description": "Testrail and ZenCrepes reporter that accepts mocha json and jest xml reports",
  "version": "0.0.1",
  "author": "Jahia",
  "bin": {
    "jahia-reporter": "./bin/run"
  },
  "bugs": "https://github.com/Jahia/jahia-reporter/issues",
  "dependencies": {
    "browser-or-node": "2.0.0",
    "@oclif/core": "^4.5.5",
    "@slack/web-api": "^7.11.0",
    "@pagerduty/pdjs": "^2.2.4",
    "@types/node": "^18",
    "@types/node-fetch": "^2.6.13",
    "@types/parse-github-url": "^1.0.3",
    "@types/uuid": "^10.0.0",
    "axios": "^1.12.2",
    "date-fns-timezone": "^0.1.4",
    "glob": "^11.0.3",
    "global": "^4.4.0",
    "google-spreadsheet": "^5.0.2",
    "js-base64": "^3.7.8",
    "load-yaml-file": "1.0.0",
    "node-fetch": "^2.7.0",
    "parse-github-url": "^1.0.3",
    "ts-node": "^10.9.2",
    "ts-sync-request": "^1.4.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "md5": "^2.3.0",
    "uuid": "^10.0.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@oclif/plugin-help": "^6.2.33",
    "@oclif/test": "^4.1.14",
    "@types/chai": "^5.2.2",
    "@types/google-spreadsheet": "^3.3.2",
    "@types/jest": "^30.0.0",
    "@types/md5": "^2.3.5",
    "chai": "^6.2.0",
    "eslint": "^8.57.1",
    "eslint-config-oclif": "^5.2.2",
    "jest": "^30.2.0",
    "nyc": "^17.1.0",
    "ts-jest": "^29.4.5"
  },
  "overrides": {
    "@pagerduty/pdjs": {
      "browser-or-node": "2.0.0"
    }
  },
  "resolutions": {
    "browser-or-node": "2.0.0"
  },  
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/Jahia/jahia-reporter",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "jahia-reporter",
    "dirname": "jahia-reporter",
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": ":",
    "topics": {
      "utils": {
        "description": "Set of small utility functions"
      }
    }
  },
  "repository": "Jahia/jahia-reporter",
  "scripts": {
    "build": "tsc -b",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif readme",
    "test": "nyc --extension .ts jest",
    "lint": "eslint . --ext .js,.ts --fix"
  },
  "types": "lib/index.d.ts"
}
